# Function to check if three lengths can form a triangle
def is_triangle(a, b, c):
    
    if a + b > c and a + c > b and b + c > a:
        print("Yes")
    else:
        print("No")

def main():
    # Get input from the user
    a = int(input("Enter the length of the first stick: "))
    b = int(input("Enter the length of the second stick: "))
    c = int(input("Enter the length of the third stick: "))
   
    # Call the is_triangle function to check the triangle condition
    is_triangle(a, b, c)

# Call the main function to run the program
main()


