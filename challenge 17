def is_power(a, b):
   
    if a < b:
        return False
    
   .
    if a == b:
        return True
    
    
    if a % b == 0:
        return is_power(a // b, b)
    
    # If a is not divisible by b, return False.
    return False

# Test cases
print(is_power(16, 2))  
print(is_power(27, 3)) 
print(is_power(9, 2))   
print(is_power(81, 3)) 
print(is_power(32, 5))  
